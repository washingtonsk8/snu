<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="contentAjuda" prefHeight="600.0" prefWidth="800.0" styleClass="main" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="snu.fronteiras.controladores.ajuda.AjudaController">
    <children>
        <ImageView fx:id="logoSistema" fitHeight="340.0" fitWidth="272.0" layoutX="46.0" layoutY="160.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../images/logo.png" />
            </image>
        </ImageView>
        <ImageView fx:id="iconeAquivoLog" fitHeight="77.0" fitWidth="151.0" layoutX="412.0" layoutY="311.0" onMouseClicked="#onClickFromIconeArquivoLog" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../images/iconeArquivoLog.png" />
            </image>
        </ImageView>      
        <ImageView fx:id="iconeSobre" fitHeight="77.0" fitWidth="148.0" layoutX="382.0" layoutY="172.0" onMouseClicked="#onClickFromIconeSobre" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../images/iconeSobre.png" />
            </image>
        </ImageView>
        <ImageView fx:id="imgVoltar" fitHeight="26.0" fitWidth="34.0" layoutX="759.0" layoutY="14.0" onMouseClicked="#onMouseClickedFromImgVoltar" pickOnBounds="true" preserveRatio="true" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
            <image>
                <Image url="@../../images/icons/iconeVoltar.png" />
            </image>
        </ImageView>      
    </children>
    <stylesheets>
        <URL value="@../../css/contents.css" />
    </stylesheets>
</AnchorPane>
