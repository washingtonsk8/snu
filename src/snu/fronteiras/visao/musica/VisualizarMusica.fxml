  </Label>
  <Label fx:id="lblAfinacao" alignment="CENTER_RIGHT" font="$x1" layoutX="14.0" layoutY="150.0" onMouseClicked="#onMouseClickedFromLblAfinacao" prefWidth="140.0" text="Afinação:" textFill="BLACK">
    <effect>
      <Glow />
