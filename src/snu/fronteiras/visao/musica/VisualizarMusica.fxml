  <Button fx:id="btnVisualizaMissasPresente" onAction="#onActionFromBtnVisualizarMissasPresente" layoutX="170.0" layoutY="440.0" mnemonicParsing="false" prefHeight="22.0" prefWidth="100.0" text="Visualizar..." />
