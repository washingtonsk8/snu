  </Label>
  <Label fx:id="lblTipos" alignment="CENTER_RIGHT" font="$x1" layoutX="14.0" layoutY="210.0" onMouseClicked="#onMouseClickedFromLblTipos" prefWidth="140.0" text="Tipos:" textFill="BLACK">
