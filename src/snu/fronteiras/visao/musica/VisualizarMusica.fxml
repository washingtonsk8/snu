<AnchorPane fx:id="contentVisualizarMusica" onMouseClicked="#onMouseClickedFromContentVisualizarMusica" prefHeight="600.0" prefWidth="800.0" styleClass="main" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="snu.fronteiras.controladores.musica.VisualizarMusicaController">
