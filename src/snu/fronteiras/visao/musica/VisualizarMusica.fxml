      <Font size="16.0" fx:id="x1" />
    </font>
  </Label>
  <Label fx:id="lblAutor" alignment="CENTER_RIGHT" font="$x1" layoutX="14.0" layoutY="60.0" onMouseClicked="#onMouseClickedFromLblAutor" prefWidth="140.0" text="Autor:" textFill="BLACK">
