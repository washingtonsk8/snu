<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="contentConfiguracoes" prefHeight="600.0" prefWidth="800.0" styleClass="main" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="snu.fronteiras.controladores.configuracoes.ConfiguracoesController">
    <children>
        <ImageView fx:id="logoSistema" fitHeight="340.0" fitWidth="272.0" layoutX="46.0" layoutY="160.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../images/logo.png" />
            </image>
        </ImageView>
        <ImageView fx:id="iconeExportarDados" fitHeight="75.0" fitWidth="163.0" layoutX="334.0" layoutY="187.0" onMouseClicked="#onClickFromIconeExportarDados" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../images/iconeExportarDados.png" />
            </image>
        </ImageView>      
        <ImageView fx:id="iconeTemplateEmail" fitHeight="94.0" fitWidth="132.0" layoutX="268.0" layoutY="75.0" onMouseClicked="#onClickFromIconeTemplateEmail" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../images/iconeTemplateEmail.png" />
            </image>
        </ImageView>
        <ImageView fx:id="iconeImportarDados" fitHeight="72.0" fitWidth="163.0" layoutX="386.0" layoutY="310.0" onMouseClicked="#onClickFromIconeImportarDados" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../images/iconeImportarDados.png" />
            </image>
        </ImageView>
        <ImageView fx:id="iconeDefinirPreferenciaTons" fitHeight="139.0" fitWidth="186.0" layoutX="334.0" layoutY="439.0" onMouseClicked="#onClickFromIconeDefinirPreferenciaTons" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../images/iconeDefinirPreferenciaTons.png" />
            </image>
        </ImageView>     
        <ImageView fx:id="imgVoltar" fitHeight="20.0" fitWidth="20.0" layoutX="761.0" layoutY="17.0" onMouseClicked="#onMouseClickedFromImgVoltar" pickOnBounds="true" preserveRatio="true" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
            <image>
                <Image url="@../../images/icons/iconeVoltar.png" />
            </image>
        </ImageView>
      <Label fx:id="lblDataImportacao" alignment="TOP_LEFT" layoutX="380.0" layoutY="381.0" prefHeight="47.0" prefWidth="176.0" text="Não ocorreram importações na versão mais recente do sistema." wrapText="true">
         <effect>
            <Glow level="0.5" />
         </effect>
      </Label>
      <Label fx:id="lblDataExportacao" alignment="TOP_LEFT" layoutX="328.0" layoutY="259.0" prefHeight="47.0" prefWidth="176.0" text="Não ocorreram exportações na versão mais recente do sistema." wrapText="true">
         <effect>
            <Glow level="0.5" />
         </effect>
      </Label> 
    </children>
    <stylesheets>
        <URL value="@../../css/contents.css" />
    </stylesheets>
</AnchorPane>
